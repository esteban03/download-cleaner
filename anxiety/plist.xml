<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>me.steban.www.anxiety</string>

    <key>ProgramArguments</key>
    <array>
        <string>{command_path}</string>
        <string>watch</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/me.steban.www.anxiety.stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/me.steban.www.anxiety.stderr.log</string>
</dict>
</plist>